{
  "name": "nanostores-rxjs",
  "version": "0.0.1",
  "description": "RXJS Support for nanostores",
  "main": "index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "keywords": [
    "rxjs",
    "nanostores",
    "typescript",
    "javascript"
  ],
  "author": "Alexandre RUIZ - Snowcodes",
  "license": "",
  "repository": {
    "type": "git",
    "url": "git@github.com:snowcodes-oss/nanostores-rxjs.git"
  },
  "files": [
    "lib/**/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "jest": "^29.3.1",
    "prettier": "^2.8.0",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.3"
  }
}
